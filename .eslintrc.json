{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"plugin:react/recommended",
		"google",
		"plugin:prettier/recommended"
	],
	"settings": {
		"import/resolver": {
			"node": {
				"paths": ["src"]
			}
		},
		"react": {
			"version": "detect"
		}
	},
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"react/prop-types": 2,
		"no-unused-vars": 2
	}
}
